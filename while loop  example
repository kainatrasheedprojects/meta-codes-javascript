var i = 1;
while (i < 6) {
    console.log(i);
    i++;
};
console.log('Counting completed!');
// while loop example 2 


var i = 5;
while (i > 0) {
    console.log(i);
    i = i - 1;
};
console.log('Counting completed!');

// while loop example
var i = 2018;
while (i < 2023) {
    console.log(i);
    i++;
};


